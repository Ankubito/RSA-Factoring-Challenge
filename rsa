#!/usr/bin/python3
from sys import argv

if len(argv) < 2:
    print("Usage: <script>.py <filename>")
    exit(1)

filename = argv[1]

with open(filename) as fd:
    for line in fd:
        number = int(line.strip())
        print("{}=".format(number), end='')

        if number % 2 == 0:
            print("{}*2".format(number // 2))
        else:
            for a in range(3, number, 2):
                if number % a == 0:
                    factor = number // a
                    for b in range(3, factor, 2):
                        if factor % b == 0:
                            break
                    else:
                        print("{}*{}".format(a, factor))
                        break

